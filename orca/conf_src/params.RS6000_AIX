# platform-dependent macros

# platform
OS		= AIX
PROC		= RS6000
BINSUF		= AIX4

# C compiler
CC		= gcc
EXT_FLAGS	= 
LDFLAGS		= 

# archiver
AR		= ar
ARFLAGS		= rv
RANLIB		= :

# lint
LINT		= /usr/bin/lint
LINTFLAGS       =
LINTPREF        = 
LINTSUF         = ln
LINTMKLIB       = -C$(LIBNAM)

# dependency generator
MAKEDEPEND      = /usr/bin/X11/makedepend
