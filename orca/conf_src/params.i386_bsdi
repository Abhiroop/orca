# platform-dependent macros

# platform
OS		= bsdi
PROC		= i386
BINSUF		= bsdi

# C compiler
CC		= gcc
EXT_FLAGS	= -O -ansi -pedantic
LDFLAGS		=

# archiver
AR 		= ar
ARFLAGS		= cr
RANLIB		= ranlib

# lint
LINT		= gcc -Wall
LINTFLAGS	=
LINTPREF	= 
LINTSUF		= 
LINTMKLIB	= 

# dependency generator
MAKEDEPEND	= /usr/X11/bin/makedepend
