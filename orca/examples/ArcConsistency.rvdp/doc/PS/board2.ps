%!PS
%%BoundingBox: 0 200 450 445

0 setgray

save
/ZapfDingbats findfont 30 scalefont setfont
450 377 moveto
gsave
-1 1 scale
<d9> show
grestore
450 251 moveto
gsave
-1 1 scale
<d9> show
grestore
restore

/queen {
        0 0 moveto
        aload
	pop
	1 sub
	5 mul
	exch
	1 sub
	5 mul
	exch
	rmoveto
	5 0 rlineto
	0 5 rlineto
	-5 0 rlineto
	0 -5 rlineto
	0 setgray
	fill
} def

/DrawBoard {
	gsave
	currentpoint translate
	0 20 rlineto
	5 0 rlineto
	0 -20 rlineto
	5 0 rlineto
	0 20 rlineto
	5 0 rlineto
	0 -20 rlineto
	5 0 rlineto
	0 20 rlineto
	-20 0 rlineto
	0 -5 rmoveto
	20 0 rlineto
	0 -5 rmoveto
	-20 0 rlineto
	0 -5 rmoveto
	20 0 rlineto
	0 -5 rmoveto
	-20 0 rlineto
	stroke
	{queen} forall
	grestore
} def

%% 1st column
0 305 moveto
[] DrawBoard

%% 2nd column
1 1 2 {dup 80 mul 185 add 100 exch moveto
	4 mod 1 add 1 exch 2 array astore
        1 array astore DrawBoard} for

%% 3rd column
0 3 3 {dup 30 mul 335 add 200 exch moveto
	4 mod 1 add 2 exch 2 array astore
        [1 3] 2 array astore DrawBoard} for
0 3 3 {dup 30 mul 200 add 200 exch moveto
	4 mod 1 add 2 exch 2 array astore
        [1 2] 2 array astore DrawBoard} for

%% 4th column
0 3 3 {dup 25 mul 330 add 300 exch moveto
	4 mod 1 add 3 exch 2 array astore
        [1 3] [2 1] 3 array astore DrawBoard} for
0 3 3 {dup 25 mul 220 add 300 exch moveto
	4 mod 1 add 3 exch 2 array astore
        [1 2] [2 4] 3 array astore DrawBoard} for

%% 5th column
1 1 2 {dup 25 mul 350 add 400 exch moveto
	4 mod 1 add 4 exch 2 array astore
        [1 3] [2 1] [3 4] 4 array astore DrawBoard} for
1 1 2 {dup 25 mul 200 add 400 exch moveto
	4 mod 1 add 4 exch 2 array astore
        [1 2] [2 4] [3 1] 4 array astore DrawBoard} for


% lines from column 1 to 2
1 1 2 {80 mul 195 add 20 315 moveto 100 exch lineto} for stroke

% lines from column 2 to 3
0 3 3 {30 mul 345 add 120 355 moveto 200 exch lineto} for stroke
0 3 3 {30 mul 210 add 120 275 moveto 200 exch lineto} for stroke

% lines from column 3 to 4
0 3 3 {25 mul 340 add 220 345 moveto 300 exch lineto} for stroke
0 3 3 {25 mul 230 add 220 300 moveto 300 exch lineto} for stroke

% lines from column 4 to 5
1 1 2 {25 mul 360 add 320 415 moveto 400 exch lineto} for stroke
1 1 2 {25 mul 210 add 320 230 moveto 400 exch lineto} for stroke

